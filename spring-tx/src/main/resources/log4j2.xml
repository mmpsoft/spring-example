<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="info" monitorInterval="30">
    <!--控制台-->
    <appenders>
        <console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </console>
    </appenders>

    <loggers>
        <root level="INFO" >
            <appenderRef ref="Console"/>
        </root>
    </loggers>
</Configuration>